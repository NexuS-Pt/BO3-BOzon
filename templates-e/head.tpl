<title>{{ sitename }}</title>

<!-- begin favicon -->
<link rel="shortcut icon" href="{{ path }}/site-assets/favicon/favicon.ico" type="image/x-icon" />
<link rel="apple-touch-icon" href="{{ path }}/site-assets/favicon/apple-touch-icon.png" />
<link rel="apple-touch-icon" sizes="57x57" href="{{ path }}/site-assets/favicon/apple-touch-icon-57x57.png" />
<link rel="apple-touch-icon" sizes="72x72" href="{{ path }}/site-assets/favicon/apple-touch-icon-72x72.png" />
<link rel="apple-touch-icon" sizes="76x76" href="{{ path }}/site-assets/favicon/apple-touch-icon-76x76.png" />
<link rel="apple-touch-icon" sizes="114x114" href="{{ path }}/site-assets/favicon/apple-touch-icon-114x114.png" />
<link rel="apple-touch-icon" sizes="120x120" href="{{ path }}/site-assets/favicon/apple-touch-icon-120x120.png" />
<link rel="apple-touch-icon" sizes="144x144" href="{{ path }}/site-assets/favicon/apple-touch-icon-144x144.png" />
<link rel="apple-touch-icon" sizes="152x152" href="{{ path }}/site-assets/favicon/apple-touch-icon-152x152.png" />
<link rel="apple-touch-icon" sizes="180x180" href="{{ path }}/site-assets/favicon/apple-touch-icon-180x180.png" />
<!-- end favicon -->

<meta property="og:site_name" content="{{ sitename }}" />
<meta property="og:type" content="website" />
<meta property="og:url" content="{{ og-url }}" />
<meta property="og:image" content="{{ og-image }}" />
<meta property="og:title" content="{{ og-title }}" />
<meta property="og:description" content="{{ og-description }}" />

<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
<meta name="keywords" content="{{ keywords }}" />
<meta name="description" content="{{ description }}" />
<meta name="robots" content="index" />
<meta name="author" content="One:Shift at one-shift.com" />
<meta name="author-code" content="Developer" />
<meta name="generator" content="BO3 {{ bo3-version }} {{ bo3-subversion }}" />

<!-- begin jquery -->
<script type="text/javascript" src="{{ libs }}/jquery-3.4.1/jquery-3.4.1.min.js"></script>
<!-- end jquery -->

<!-- begin bootstrap -->
<script type="text/javascript" src="{{ libs }}/popper-1.14.7/popper.min.js"></script>
<script type="text/javascript" src="{{ libs }}/bootstrap-4.3.1/js/bootstrap.min.js"></script>
<link rel="stylesheet" href="{{ libs }}/bootstrap-4.3.1/css/bootstrap.min.css">
<!-- end bootstrap -->

<!-- begin fontawesome -->
<link rel="stylesheet" href="{{ libs }}/fontawesome-free-5.6.3/css/all.css">
<!-- end fontawesome -->

<script type="text/javascript" src="{{ path }}/site-assets/js/script.js"></script>
<link rel="stylesheet" href="{{ path }}/site-assets/css/style.css" type="text/css" media="screen" />
<link rel="stylesheet" href="{{ path }}/site-assets/css/spacers.min.css" type="text/css" media="screen" />

<script>
	var path = "{{ path }}";
	var lg = "{{ lg }}";
</script>

{{ analytics }}
